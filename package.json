{
  "name": "@trufnetwork/sdk-js",
  "version": "0.3.2",
  "description": "Truf Network SDK for JavaScript",
  "type": "module",
  "keywords": [
    "truf",
    "truflation",
    "sdk",
    "javascript",
    "typescript",
    "web3"
  ],
  "author": "",
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "node": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.node.cjs",
      "import": "./dist/esm/index.node.mjs"
    },
    "browser": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.browser.cjs",
      "import": "./dist/esm/index.browser.mjs"
    },
    "default": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsx ./scripts/esbuild.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build",
    "tag:dev": "pnpm version 0.0.0-dev-$(date +%Y%m%d%H%M%S) --no-git-tag-version",
    "publish:dev": "pnpm publish --tag dev --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@kwilteam/kwil-js": "^0.9.4",
    "crypto-hash": "^3.1.0",
    "ethers": "^6.13.5",
    "lodash": "^4.17.21",
    "monads-io": "^4.0.2",
    "uuid": "^11.1.0",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@types/lodash": "4.17.12",
    "@types/node": "18.18.1",
    "@types/pg": "^8.11.13",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "disposablestack": "1.1.6",
    "esbuild": "0.24.0",
    "esbuild-plugin-file-path-extensions": "^2.1.3",
    "esbuild-plugin-import-assertions": "1.0.1",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "glob": "11.0.0",
    "prettier": "3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite-plugin-plain-text": "1.4.2",
    "vitest": "^2.1.3"
  }
}
